<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- 데이터베이스 컬럼명 A_COLUMN을 CamelCase형태의 자바 프로퍼트로 매핑 aColumn -->
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	<typeAliases>
		<!-- type별칭 VO이름을 간략히 사용 -->
		<typeAlias type="com.vvs.shop.TestVO" alias="TestVO" />
		<typeAlias type="com.vvs.shop.file.FileVO" alias="FileVO" />
		<typeAlias type="com.vvs.shop.member.MemberVO" alias="MemberVO" />
		<typeAlias type="com.vvs.shop.product.ProductVO" alias="ProductVO" />
		<typeAlias type="com.vvs.shop.cart.CartVO" alias="CartVO" />
		<typeAlias type="com.vvs.shop.orders.OrdersVO" alias="OrdersVO" />
		<typeAlias type="com.vvs.shop.orders.OrdersProductVO" alias="OrdersProductVO" />
		<typeAlias type="com.vvs.shop.ship.ShipVO" alias="ShipVO" />
		<typeAlias type="com.vvs.shop.cmn.SearchVO" alias="SearchVO"/>
		<typeAlias type="com.vvs.shop.product.ProductDetailVO" alias="ProductDetailVO"/>
		<typeAlias type="com.vvs.shop.product.OptionsVO" alias="OptionsVO"/>
	</typeAliases>
</configuration>